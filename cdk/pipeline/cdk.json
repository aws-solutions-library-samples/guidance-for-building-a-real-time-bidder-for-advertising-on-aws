  "app": "python3 app.py",
  "context": {
    "dev": {
      "AWS_ACCOUNT_ID": "",
      "RTBKIT_ROOT_STACK_NAME": "<rtbkit-dev>",
      "RTBKIT_VARIANT": "<DynamoDB/Aerospike>"
    },
    "staging": {
      "AWS_ACCOUNT_ID": "<>",
      "RTBKIT_ROOT_STACK_NAME": "<rtbkit-bkr-stg>",
      "RTBKIT_VARIANT": "<DynamoDB/Aerospike>"
